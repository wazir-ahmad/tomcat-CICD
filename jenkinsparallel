pipeline
{ 
agent  any
stages 
{
stage('scm checkout')
{
steps{
git branch: 'master', url: 'https://github.com/wazir-ahmad/maven-project.git'
}
}
stage ('parallel deployment')
{
parallel {
stage('test')
{
steps {
withMaven(jdk: 'localjdk-1.8', maven: 'localmaven'){
sh 'mvn test'
}
}

stage ('package')
{
steps {
withMaven(jdk: 'localjdk-1.8', maven: 'localmaven') {
sh 'mvn package'
}
}
}
failFast true
}
}
}

