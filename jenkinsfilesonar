pipeline
{ 
agent  any
stages
{
stage('scm checkout')
{
steps {
git branch: 'master', url: 'https://github.com/wazir-ahmad/maven-project.git'
}
}
stage ('validate code')
{
steps
{
withSonarQubeEnv('sonar') {
withMaven(jdk: 'localjdk-1.8', maven: 'localmaven') {
    sh 'mvn install sonar:sonar'}
 }
}
}
}
}

